// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  username String @id // @default(cuid())
  password String
  balance Int
  name String
  surname String
}

model Book {
  isbn Int @id
  title String?
  author String?
  publisher String?
  publish_year Int?
  genre String?
  price Int?
  quantity Int?
}
